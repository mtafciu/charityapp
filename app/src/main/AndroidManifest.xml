<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ghost.todaydifferent">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".MainPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".ShopActivity"></activity>
        <activity android:name=".VolunteerActivity"></activity>
        <activity android:name=".DonationActivity"></activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".InterestsActivity"></activity>
        <activity android:name=".VolunteerProjectActivity"></activity>
    </application>

</manifest>
